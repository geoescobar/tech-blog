<header id="navbar">
  <nav class="navbar navbar-expand-lg bg-none">
    <div class="container-fluid">
      <h3>Tech Blog</h3>
      <ul class="navbar-nav">
        <li class="nav-item">
          <a
            id="#"
            class="text-light nav-link active"
            aria-current="page"
            href="/feed"
          >Feed</a>
        </li>
        <li class="nav-item">
          <a
            id="#logout"
            class="text-light nav-link active"
            aria-current="page"
            href="/login"
          >Logout</a>
        </li>
      </ul>
    </div>
  </nav>
</header>

<div class="container feed">
  <div class="row justify-content-between">
    <div class="col-8 posts">
      {{>myPosts}}
    </div>
    <div class="col-4">
      <div class="d-flex justify-content-center">
        <div class="card my-card" style="width: 50rem;">
          <div class="card-body">
            <h5 class="card-subtitle mb-2 my-username">username</h5>
            <h6 class="card-title text-dark my-posts">My Posts</h6>
            <p class="card-text text-dark instructions">You can edit and delete
              any of your posts on this page.</p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>