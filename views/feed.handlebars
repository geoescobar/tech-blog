{{! header }}
<header id="navbar">
  <nav class="navbar navbar-expand-lg bg-none">
    <div class="container-fluid">
      <h3>Tech Blog</h3>
      <ul class="navbar-nav">
        <li class="nav-item">
          <a
            id="#"
            class="text-light nav-link active"
            aria-current="page"
            href="/homepage"
          >Profile</a>
        </li>
        <li class="nav-item">
          <a
            id="logout"
            class="text-light nav-link active"
            aria-current="page"
            href="/login"
          >Logout</a>
        </li>
      </ul>
    </div>
  </nav>
</header>


<div class="container feed">
  <div class="row justify-content-between">
    <div class="col-8 posts">
      {{#each posts}}
        <div class="d-flex justify-content-center">
          <div class="card card-container text-dark" style="width: 50rem;">
            <div class="card-body card-title">
              <h3>{{postTitle}}</h3>
              <p class="username">@{{user.username}}</p>
            </div>
            <div class="card-body card-post">
              <p>{{postBody}}</p>
            </div>
          </div>
        </div>
      {{/each}}    
    </div>
    <div class="col-4">
      {{> posts }}
    </div>
  </div>
</div>





